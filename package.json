    "minisearch": "3.0.2",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-autosuggest": "10.1.0",
    "react-dom": "17.0.1",
    "react-final-form": "6.5.2",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "react-sortable-hoc": "1.11.0",
    "react-spinners": "0.10.4",
    "react-syntax-highlighter": "15.4.3",
